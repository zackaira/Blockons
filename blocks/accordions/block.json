{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "blockons/accordions",
	"title": "Accordions",
	"textdomain": "blockons",
	"icon": "menu",
	"category": "blockons-category",
	"keywords": ["accordions", "accordionbed", "content", "inner"],
	"supports": {
		"html": false
	},
	"attributes": {
		"labelTag": {
			"type": "string",
			"default": "h4"
		},
		"accordionDesign": {
			"type": "string",
			"default": "one"
		},
		"iconFirst": {
			"type": "boolean",
			"default": false
		},
		"closeAll": {
			"type": "boolean",
			"default": false
		},
		"centeredLabel": {
			"type": "boolean",
			"default": false
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewScript": "file:./js-accordions.js",
	"style": ["file:./style-index.css", "blockons-fontawesome"]
}
