{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "blockons/line-heading",
	"title": "Block-Ons Line Heading",
	"textdomain": "blockons",
	"icon": "heading",
	"category": "blockons-category",
	"keywords": ["line", "heading", "title"],
	"attributes": {
		"alignment": {
			"type": "string",
			"default": "left"
		},
		"headElementAlign": {
			"type": "string",
			"default": "left"
		},
		"hTag": {
			"type": "string",
			"default": "h3"
		},
		"headWidthSet": {
			"type": "string",
			"default": "outer"
		},
		"headOuterWidth": {
			"type": "number",
			"default": 100
		},
		"headLineWidth": {
			"type": "number",
			"default": 100
		},
		"headingTitle": {
			"type": "string",
			"default": ""
		},
		"lineWidth": {
			"type": "number",
			"default": 2
		},
		"lineStyle": {
			"type": "string",
			"default": "solid"
		},
		"headVertAlign": {
			"type": "string",
			"default": "centeralign"
		},
		"fontColor": {
			"type": "string",
			"default": "#4f4f4f"
		},
		"lineColor": {
			"type": "string",
			"default": "#4f4f4f"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": ["file:./style-index.css", "blockons-fontawesome"]
}
