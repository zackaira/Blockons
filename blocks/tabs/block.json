{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "blockons/tabs",
	"title": "Tabs",
	"textdomain": "blockons",
	"icon": "table-row-after",
	"category": "blockons-category",
	"keywords": ["tabs", "tabbed", "content", "inner"],
	"supports": {
		"html": false
	},
	"attributes": {
		"tabs": {
			"type": "array",
			"default": []
		},
		"tabDesign": {
			"type": "string",
			"default": "one"
		},
		"alignment": {
			"type": "string",
			"default": "center"
		},
		"tabsSideLayout": {
			"type": "boolean",
			"default": false
		},
		"tabsJustified": {
			"type": "boolean",
			"default": false
		},
		"contentMinHeight": {
			"type": "number",
			"default": 100
		},
		"tabWidth": {
			"type": "number",
			"default": 200
		},
		"tabMinWidth": {
			"type": "number",
			"default": 120
		},
		"tabBorderRadius": {
			"type": "number",
			"default": 4
		},
		"tabVertPadding": {
			"type": "number",
			"default": 8
		},
		"tabHorizPadding": {
			"type": "number",
			"default": 16
		},
		"contentVertPadding": {
			"type": "number",
			"default": 1
		},
		"contentHorizPadding": {
			"type": "number",
			"default": 20
		},
		"activeColor": {
			"type": "string",
			"default": "#FFF"
		},
		"activeFontColor": {
			"type": "string",
			"default": "#000"
		},
		"selectedColor": {
			"type": "string",
			"default": "#000"
		},
		"selectedFontColor": {
			"type": "string",
			"default": "#FFF"
		},
		"nonActiveColor": {
			"type": "string",
			"default": "#ececec"
		},
		"nonActiveFontColor": {
			"type": "string",
			"default": "#000"
		},
		"contentColor": {
			"type": "string",
			"default": "inherit"
		},
		"otherContentColor": {
			"type": "string",
			"default": "#f7f7f7"
		},
		"contentFontColor": {
			"type": "string",
			"default": "inherit"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"script": "file:./js-tabs.js",
	"style": ["file:./style-index.css", "blockons-fontawesome"]
}
