{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "blockons/cart-icon",
	"title": "Block-Ons Cart Icon",
	"textdomain": "blockons",
	"icon": "cart",
	"category": "blockons-category",
	"attributes": {
		"alignment": {
			"type": "string",
			"default": "none"
		},
		"hasDropdown": {
			"type": "boolean",
			"default": false
		},
		"icon": {
			"type": "string",
			"default": "fa-solid fa-cart-shopping"
		},
		"iconSize": {
			"type": "number",
			"default": 17
		},
		"iconPadding": {
			"type": "number",
			"default": 8
		},
		"customIcon": {
			"type": "string",
			"default": "fa-regular fa-face-smile"
		},
		"iconBgColor": {
			"type": "string",
			"default": "#FFF"
		},
		"iconColor": {
			"type": "string",
			"default": "#000"
		},
		"dropBgColor": {
			"type": "string",
			"default": "#FFF"
		},
		"dropColor": {
			"type": "string",
			"default": "#747474"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"script": "blockons-cart-icon",
	"style": ["file:./style-index.css", "blockons-fontawesome"]
}
