{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "blockons/icon-list",
	"title": "Block-Ons Icon List",
	"textdomain": "blockons",
	"icon": "admin-users",
	"category": "blockons-category",
	"attributes": {
		"alignment": {
			"type": "string",
			"default": "none"
		},
		"listItems": {
			"type": "array",
			"default": [
				{
					"itemId": 0,
					"itemText": "List Item",
					"itemIcon": "check"
				},
				{
					"itemId": 1,
					"itemText": "List Item",
					"itemIcon": "check"
				}
			]
		},
		"listItemSpacing": {
			"type": "number",
			"default": 3
		},
		"listItemIconSpacing": {
			"type": "number",
			"default": 5
		},
		"listItemIconSize": {
			"type": "number",
			"default": 18
		},
		"listItemFontSize": {
			"type": "number",
			"default": 16
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": ["file:./style-index.css", "blockons-fontawesome"]
}
