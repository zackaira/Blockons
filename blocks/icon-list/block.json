{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "blockons/icon-list",
	"title": "Block-Ons Icon List",
	"textdomain": "blockons",
	"icon": "editor-ul",
	"category": "blockons-category",
	"keywords": ["list", "icons", "items", "links"],
	"attributes": {
		"alignment": {
			"type": "string",
			"default": "none"
		},
		"listItems": {
			"type": "array",
			"default": [
				{
					"itemId": 0,
					"itemText": "",
					"itemTextSize": null,
					"itemTextColor": null,
					"itemIcon": "check",
					"itemIconSize": null,
					"itemIconColor": null
				},
				{
					"itemId": 1,
					"itemText": "",
					"itemTextSize": null,
					"itemTextColor": null,
					"itemIcon": "check",
					"itemIconSize": null,
					"itemIconColor": null
				}
			]
		},
		"listItemsLayout": {
			"type": "string",
			"default": "vertical"
		},
		"listItemSpacing": {
			"type": "number",
			"default": 8
		},
		"listItemIconSpacing": {
			"type": "number",
			"default": 5
		},
		"listItemIconSize": {
			"type": "number",
			"default": 18
		},
		"listItemIconColor": {
			"type": "string",
			"default": "#4f4f4f"
		},
		"listItemFontSize": {
			"type": "number",
			"default": 16
		},
		"listItemFontColor": {
			"type": "string",
			"default": "#4f4f4f"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": ["file:./style-index.css", "blockons-fontawesome"]
}
